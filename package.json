{
  "name": "nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1643791578.532b3d6",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/i18n": "^7.2.0",
    "@tinymce/tinymce-vue": "^3",
    "@vueuse/core": "^8.2.4",
    "@vueuse/nuxt": "^8.2.4",
    "apexcharts": "^3.35.5",
    "core-js": "^3.19.3",
    "flag-icons": "^6.6.6",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "object-to-formdata": "^4.4.2",
    "portal-vue": "^2.1.7",
    "v-currency-field": "^3.1.1",
    "v-phone-input": "^2.0.0",
    "vee-validate": "^3.4.14",
    "vue": "^2.6.14",
    "vue-apexcharts": "^1.6.2",
    "vue-condition-watcher": "^1.4.3",
    "vue-currency-filter": "^5.2.0",
    "vue-server-renderer": "^2.6.14",
    "vue-simple-acl": "^1.2.0",
    "vue-template-compiler": "^2.6.14",
    "vue-toastification": "^1.7.14",
    "vue-virtual-scroll-list": "^2.3.3",
    "vuetify": "^2.6.1",
    "vuetify-dialog": "^2.0.17",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^5.0.0-4",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/lodash": "^4.14.179",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "prettier-eslint-cli": "^5.0.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0"
  }
}
